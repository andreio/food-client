{
  "name": "food",
  "private": true,
  "main": "index.js",
  "version": "0.0.1",
  "scripts": {
    "rn-cli": "node scripts/react-native.js",
    "start:android": "yarn rn-cli run-android",
    "start:windows": "yarn rn-cli run-windows",
    "start:ios": "yarn rn-cli run-ios",
    "start:web": "cross-env platform=web webpack-dev-server --config=web/webpack/dev.js --progress --colors --mode=development",
    "start:rn-dev-server": "yarn rn-cli start --reset-cache",
    "build:web": "cross-env platform=web webpack --config=web/webpack/prod.js --progress --colors --mode=production",
    "test": "jest -c jest/jest.config.js",
    "test:watch": "yarn test --watch",
    "test:debug": "node --inspect-brk node_modules/.bin/jest -c jest/jest.config.js --runInBand",
    "build:types": "tsc --emitDeclarationOnly",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check -w",
    "lint": "eslint --config .eslintrc --ext .ts,.tsx src",
    "generate:interfaces": "graphql-codegen init",
    "gen:interfaces": "graphql-codegen --config codegen.yml"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/plugin-proposal-decorators": "7.6.0",
    "@babel/preset-env": "7.6.3",
    "@graphql-codegen/cli": "^1.8.2",
    "@graphql-codegen/fragment-matcher": "1.8.2",
    "@graphql-codegen/introspection": "1.8.2",
    "@graphql-codegen/typescript": "1.8.2",
    "@graphql-codegen/typescript-graphql-files-modules": "1.8.2",
    "@graphql-codegen/typescript-operations": "1.8.2",
    "@graphql-codegen/typescript-react-apollo": "^1.8.2",
    "@react-native-community/cli": "1.10.0",
    "@types/enzyme": "3.10.3",
    "@types/graphql": "^14.5.0",
    "@types/jest": "24.0.19",
    "@typescript-eslint/eslint-plugin": "2.5.0",
    "@typescript-eslint/parser": "2.5.0",
    "apollo": "^2.21.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "8.0.6",
    "compression-webpack-plugin": "3.0.0",
    "cross-env": "6.0.3",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "enzyme-to-json": "3.4.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-loader": "3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "22.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-reactxp": "0.1.6",
    "fork-ts-checker-webpack-plugin": "1.5.1",
    "graphql-code-generator": "^0.18.2",
    "graphql-codegen-typescript-react-apollo-template": "^0.18.2",
    "graphql-tag": "^2.10.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.9.0",
    "metro-react-native-babel-preset": "0.56.0",
    "prettier": "^1.18.2",
    "rnpm-plugin-windows": "0.3.7",
    "typescript": "3.6.4",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.8.2",
    "webpack-merge": "4.2.2"
  },
  "dependencies": {
    "@apollo/react-hoc": "^3.1.3",
    "@apollo/react-hooks": "^3.1.3",
    "apollo-boost": "^0.4.4",
    "graphql": "^14.5.8",
    "graphql.macro": "^1.4.2",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-native": "0.59.10",
    "react-native-windows": "0.59.0-rc.3",
    "reactxp": "2.0.0-rc.1"
  }
}
